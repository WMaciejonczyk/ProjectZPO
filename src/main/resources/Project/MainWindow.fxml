<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.Pane?>
<Pane fx:controller="Project.MainController"
      xmlns:fx="http://javafx.com/fxml">
    <padding><Insets top="15" right="25" bottom="15" left="25"/></padding>

    <Button fx:id="showAddMaterialMenu" text="Add material" layoutX="45" layoutY="60"
                  onAction="#handleShowFieldsMaterials"/>

    <Button fx:id="showAddEquipmentMenu" text="Add equipment" layoutX="40" layoutY="110"
            onAction="#handleShowFieldsEquipment"/>

    <Button fx:id="showAddMedicineMenu" text="Add medicine" layoutX="45" layoutY="160"
            onAction="#handleShowFieldsMedicine"/>

    <Button fx:id="showStorageMenu" text="Show storage" layoutX="105" layoutY="220" alignment="CENTER"
            onAction="#showStorage"/>
</Pane>